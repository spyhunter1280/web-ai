{
  "name": "@visheratin/web-ai",
  "version": "1.0.5",
  "description": "Run modern deep learning models in the browser and Node.js.",
  "type": "module",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "common/**/*.d.ts",
    "common/**/*.js",
    "common/**/*.js.map",
    "image/**/*.d.ts",
    "image/**/*.js",
    "image/**/*.js.map",
    "multimodal/**/*.d.ts",
    "multimodal/**/*.js",
    "multimodal/**/*.js.map",
    "text/**/*.d.ts",
    "text/**/*.js",
    "text/**/*.js.map",
    "common.js",
    "common.js.map",
    "common.d.ts",
    "image.js",
    "image.js.map",
    "image.d.ts",
    "index.js",
    "index.js.map",
    "index.d.ts",
    "multimodal.js",
    "multimodal.js.map",
    "multimodal.d.ts",
    "text.js",
    "text.js.map",
    "text.d.ts"
  ],
  "exports": {
    ".": null,
    "./common": {
      "types": "./common.d.ts",
      "import": "./common.js"
    },
    "./image": {
      "types": "./image.d.ts",
      "import": "./image.js"
    },
    "./multimodal": {
      "types": "./multimodal.d.ts",
      "import": "./multimodal.js"
    },
    "./text": {
      "types": "./text.d.ts",
      "import": "./text.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "sh build.sh",
    "lint": "eslint \"**/*.ts*\""
  },
  "dependencies": {
    "onnxruntime-common": "1.15.1"
  },
  "peerDependencies": {
    "jimp": "^0.16.2"
  },
  "peerDependenciesMeta": {
    "jimp": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "prettier": "^2.8.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visheratin/web-ai.git"
  },
  "keywords": [
    "deep-learning",
    "AI",
    "NLP",
    "CV",
    "computer-vision"
  ],
  "author": "Alexander Visheratin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visheratin/web-ai/issues"
  },
  "homepage": "https://github.com/visheratin/web-ai#readme"
}