{
  "name": "@visheratin/web-ai",
  "version": "0.7.1",
  "description": "Run modern deep learning models in the browser.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visheratin/web-ai.git"
  },
  "keywords": [
    "deep-learning",
    "AI",
    "NLP",
    "CV",
    "computer-vision"
  ],
  "author": "Alexander Visheratin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visheratin/web-ai/issues"
  },
  "homepage": "https://github.com/visheratin/web-ai#readme",
  "dependencies": {
    "@visheratin/tokenizers": "0.1.5",
    "comlink": "4.3.1",
    "localforage": "1.10.0",
    "onnxruntime-web": "1.13.1",
    "pako": "2.1.0"
  },
  "peerDependencies": {
    "jimp": "0.16.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3"
  }
}
